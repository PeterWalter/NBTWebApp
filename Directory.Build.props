<Project>
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <LangVersion>13.0</LangVersion>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>$(NoWarn);CS8618;NU1903;NU1902;IDE0090;IDE0036;IDE0044</NoWarn>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <AnalysisLevel>latest</AnalysisLevel>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>NBT Development Team</Authors>
    <Company>National Benchmark Tests</Company>
    <Product>NBT Website</Product>
    <Copyright>Copyright Â© 2025 National Benchmark Tests</Copyright>
    <PackageProjectUrl>https://www.nbt.ac.za</PackageProjectUrl>
  </PropertyGroup>
</Project>
